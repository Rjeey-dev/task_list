{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aliaksandr_abashyn/service_bus": "dev-master",
        "doctrine/doctrine-bundle": "2.4.x-dev",
        "doctrine/mongodb-odm": "2.3.x-dev",
        "doctrine/mongodb-odm-bundle": "4.2.x-dev",
        "jms/serializer-bundle": "3.9.x-dev",
        "nelmio/cors-bundle": "^2.1",
        "psr/cache": "1.0.1",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Kernel\\": "src/Kernel/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    },
    "repositories":[
        {
            "type": "vcs",
            "url" : "git@github.com:Travelix/service_bus.git"
        }
    ]
}
